class TaskService {
  final _db = FirebaseFirestore.instance;

  Future<void> addTask({
    required String title,
    required String details,
    required String subject,
    required String type,
    required DateTime dueDate,
  }) async {
    // ðŸ‘‡ 2 lines ABOVE where user-based saving starts
